generator client {
  provider = "prisma-client-js"
  output = "../build"

}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_URL_USER_PROFILE_SERVICE")
}
enum Role{
  ADMIN
  NON_ADMIN
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String @unique
  email String @unique
  password String
  role   Role   @default(NON_ADMIN)
  totalSolved Int @default(0)
}
